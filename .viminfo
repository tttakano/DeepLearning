	def sequence_embed(embed, xs):
	    x_len = [len(x) for x in xs]
	    x_section = numpy.cumsum(x_len[:-1])
	    ex = embed(F.concat(xs, axis=0))
	    exs = F.split_axis(ex, x_section, 0)
	    return exs
	
	
